
<div class="container">
  <h1 class="text-center">Favorite Pokemon</h1>

  <% pokemons.forEach(function(pokemon) { %>
    <div>
      <h2><a href="/pokemon/<%= pokemon.name %>"><%= pokemon.name %></h2>
      <form method="POST" class="delete" action="/pokemon/<%= pokemon.name %>">
        <button class="btn btn-primary" type="submit">Delete</button>
      </form>
      <hr>
    </div>
  <% }); %>
</div>